{
  "name": "ts-express-server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "ts-node": "3.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql": "^2.14.1",
    "nodemon": "^2.0.12",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.34"
  },
  "scripts": {
    "dev": "yarn typeorm migration:run && nodemon -L",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "diagram": "pipenv run python ./diagram/diagram.py && mv web_service.png diagram/",
    "prepare": "husky install",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lints": "yarn eslint && yarn prettier --write .",
    "lint-staged": "yarn lint && yarn eslint && prettier --write --ignore-unknown"
  },
  "lint-staged": {
    "**/*": "yarn lints"
  }
}
